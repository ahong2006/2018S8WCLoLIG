int doDuration = 500;

void setup() {
  // put your setup code here, to run once:
pinMode(LED_BUILTIN, OUTPUT);
}

void loop() {
 

  // A
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration*3); 
  // N
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration); 

  // G
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration); 

   //E
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration); 


   //L
   digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration);
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration*3); 
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration);
  digitalWrite(LED_BUILTIN, HIGH);  // turn the LED on (HIGH is the voltage level)
  delay(doDuration); 
  digitalWrite(LED_BUILTIN, LOW);  // turn the LED on (HIGH is the voltage level)           
  delay(doDuration*5);
